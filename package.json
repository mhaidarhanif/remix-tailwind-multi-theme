{
  "private": true,
  "name": "remix-tailwind-multi-theme",
  "description": "",
  "license": "",
  "scripts": {
    "clean": "rm -rf build public/build",
    "build": "npm run clean && remix build",
    "dev": "npm run clean && run-p dev:*",
    "dev:remix": "remix dev",
    "dev:css": "tailwindcss --watch -o ./app/styles/tailwind.css",
    "start": "cross-env NODE_ENV=production npm run build && remix-serve build"
  },
  "dependencies": {
    "@heroicons/react": "1.0.6",
    "@remix-run/node": "^1.6.7",
    "@remix-run/react": "^1.6.7",
    "@remix-run/serve": "^1.6.7",
    "@tailwindcss/typography": "0.5.4",
    "clsx": "1.2.1",
    "cross-env": "7.0.3",
    "isbot": "3.5.0",
    "npm-run-all": "4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "3.1.7"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.7",
    "@remix-run/eslint-config": "^1.6.7",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.21.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "keywords": []
